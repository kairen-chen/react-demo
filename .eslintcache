ion={background || location}>\n        <Route exact path=\"/ModalGallery\" children={<Home />} />\n        <Route path=\"/ModalGallery/gallery\" children={<Gallery />} />\n        <Route path=\"/ModalGallery/img/:id\" children={<ImageView />} />\n      </Switch>\n\n      {/* Show the modal when a background page is set */}\n      {background && <Route path=\"/ModalGallery/img/:id\" children={<Modal />} />}\n    </div>\n  );\n}\n\nconst IMAGES = [\n  { id: 0, title: \"Dark Orchid\", color: \"DarkOrchid\" },\n  { id: 1, title: \"Lime Green\", color: \"LimeGreen\" },\n  { id: 2, title: \"Tomato\", color: \"Tomato\" },\n  { id: 3, title: \"Seven Ate Nine\", color: \"#789\" },\n  { id: 4, title: \"Crimson\", color: \"Crimson\" }\n];\n\nfunction Thumbnail({ color }) {\n  return (\n    <div\n      style={{\n        width: 50,\n        height: 50,\n        background: color\n      }}\n    />\n  );\n}\n\nfunction Image({ color }) {\n  return (\n    <div\n      style={{\n        width: \"100%\",\n        height: 600,\n        background: color\n      }}\n    />\n  );\n}\n\nfunction Home() {\n  return (\n    <div>\n      <Link to=\"/ModalGallery/gallery\">Visit the Gallery</Link>\n      <h2>Featured Images</h2>\n      <ul>\n        <li>\n          <Link to=\"/ModalGallery/img/2\">Tomato</Link>\n        </li>\n        <li>\n          <Link to=\"/ModalGallery/img/4\">Crimson</Link>\n        </li>\n      </ul>\n    </div>\n  );\n}\n\nfunction GoBack() {\n  let history = useHistory();\n  return (\n    <div>\n      <button type=\"button\" onClick={()=>{\n        history.push({\n            pathname: '/ModalGallery'\n        })\n      }}>\n          上一頁\n      </button>\n    </div>\n  )\n}\n\nfunction Gallery() {\n  let location = useLocation();\n  return (\n    <div>\n      {IMAGES.map(i => (\n          <Link\n            key={i.id}\n            to={{\n              pathname: `/ModalGallery/img/${i.id}`,\n              // This is the trick! This link sets\n              // the `background` in location state.\n              state: { background: location }\n            }}\n            style={{\n              display:\"flex\",\n              margin: \"20px\"\n            }}\n          >\n            <Thumbnail color={i.color} />\n            <p \n              style={{\n                margin: \"20px\"\n              }}\n            >{i.title}</p>\n          </Link>\n      ))}\n      <GoBack />\n    </div>\n  );\n}\n\nfunction ImageView() {\n  let { id } = useParams();\n  let image = IMAGES[parseInt(id, 10)];\n\n  if (!image) return <div>Image not found</div>;\n\n  return (\n    <div>\n      <h1>{image.title}</h1>\n      <Image color={image.color} />\n      <GoBack />\n    </div>\n  );\n}\n\nfunction Modal() {\n  let history = useHistory();\n  let { id } = useParams();\n  let image = IMAGES[parseInt(id, 10)];\n\n  if (!image) return null;\n\n  let back = e => {\n    e.stopPropagation();\n    history.push({\n      pathname: '/ModalGallery/gallery',\n  });\n  };\n\n  return (\n    <div\n      style={{\n        position: \"absolute\",\n        top: 0,\n        left: 0,\n        bottom: 0,\n        right: 0,\n        background: \"rgba(0, 0, 0, 0.55)\",\n        display: \"flex\",\n        alignItems: \"center\"\n      }}\n    >\n      <div\n        className=\"modal\"\n        style={{\n          position: \"absolute\",\n          background: \"#fff\",\n          left: \"10%\",\n          right: \"10%\",\n          padding: 15,\n          border: \"2px solid #444\",\n          borderRadius: \"10px 50px\",\n          transform: \"rotate( -1deg )\"\n        }}\n      >\n        <h1>{image.title}</h1>\n        <Image color={image.color} />\n        <button type=\"button\" onClick={back}>\n          Close\n        </button>\n      </div>\n    </div>\n  );\n}\n",{"ruleId":null,"fatal":true,"severity":2,"message":"92","line":12,"column":2},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"93","replacedBy":"97"},{"ruleId":"95"